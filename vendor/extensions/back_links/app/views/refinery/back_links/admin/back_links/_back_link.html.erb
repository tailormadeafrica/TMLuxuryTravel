<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(back_link) -%>">
  <span class='title'>
    <%= back_link.old_link %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.back_links_back_link_path(back_link),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_back_links_admin_back_link_path(back_link),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.back_links_admin_back_link_path(back_link),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => back_link.old_link),
        :method => :delete %>
  </span>
</li>
